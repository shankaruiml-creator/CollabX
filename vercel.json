{
  "version": 2,
  "builds": [
    {
      "src": "target/CollabX-0.0.1-SNAPSHOT.jar",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "target"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
